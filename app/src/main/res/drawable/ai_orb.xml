<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size android:width="260dp" android:height="260dp"/>
            <gradient android:type="radial" android:gradientRadius="160dp"
                android:startColor="#2220E5FF" android:centerColor="#5520E5FF" android:endColor="#0020E5FF"/>
        </shape>
    </item>
    <item android:top="18dp" android:bottom="18dp" android:left="18dp" android:right="18dp">
        <shape android:shape="oval">
            <stroke android:width="2dp" android:color="#66FFFFFF"/>
            <solid android:color="#00000000"/>
        </shape>
    </item>
    <item android:top="36dp" android:bottom="36dp" android:left="36dp" android:right="36dp">
        <shape android:shape="oval">
            <stroke android:width="2dp" android:color="#66AEEAFF"/>
            <solid android:color="#00000000"/>
        </shape>
    </item>
    <item android:top="54dp" android:bottom="54dp" android:left="54dp" android:right="54dp">
        <shape android:shape="oval">
            <gradient android:type="sweep"
                android:startColor="#FF00E5FF" android:centerColor="#FF2E7DFF" android:endColor="#FF00E5FF"/>
            <stroke android:width="2dp" android:color="#66FFFFFF"/>
            <solid android:color="#00000000"/>
        </shape>
    </item>
    <item android:top="84dp" android:bottom="84dp" android:left="84dp" android:right="84dp">
        <shape android:shape="oval">
            <gradient android:type="radial" android:gradientRadius="60dp"
                android:startColor="#9920E5FF" android:centerColor="#5520E5FF" android:endColor="#1020E5FF"/>
            <stroke android:width="1dp" android:color="#88FFFFFF"/>
        </shape>
    </item>
</layer-list>
